<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.view"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" 
        android:debuggable="true">
        <activity
            android:name="example.view.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="example.view.EndOfGameActivity"
            android:label="@string/title_activity_end" >
        </activity>
        
         <activity
            android:name="example.view.GameActivityTime"
            android:label="@string/title_activity_game_time" >
        </activity>
        
         <activity
            android:name="example.view.GameActivityMove"
            android:label="@string/title_activity_game_move" >
        </activity>
        
        <activity
            android:name="example.view.ScoresActivity"
            android:label="@string/title_activity_scores" >
        </activity>
        
        <activity
            android:name="example.view.ExitActivity"
            android:label="@string/title_activity_exit" >
        </activity>
        
        <activity
            android:name="example.view.ModeActivity"
            android:label="@string/title_activity_mode" >
        </activity>
        
    </application>

</manifest>